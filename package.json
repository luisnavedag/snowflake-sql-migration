    "chai": "^4.3.0",
    "chai-as-promised": "^7.1.1",
    "chai-subset": "^1.6.0",
    "del-cli": "^3.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "mocha": "^8.3.0",
    "nyc": "^15.1.0",
    "sinon": "^9.2.4",
